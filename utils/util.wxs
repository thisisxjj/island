var filter = function(arr, n) {
  if(!arr) {
    return
  }
  var temp = []
  for(var i = 0; i < n; i++) {
    temp[i] = arr[i]
  }
  return temp
}
//拼接歌手数组中歌手的名字
var splice = function(arr) {
  if(!arr) {
    return
  }
  var temp = ''
  for(var i = 0; i < arr.length; i++) {
    temp += (i < arr.length - 1) ? (arr[i].name + '/') : arr[i].name 
  }
  return temp
}
module.exports = {
  filter: filter,
  splice: splice
}