<wxs src="../../utils/util.wxs" module="tools"></wxs>
<view class="container">
  <image class="image" src="{{src}}" />
  <view class="right-container">
    <block wx:for="{{songlist}}" wx:key="index">
      <text class="text">
        {{index + 1}}.{{item.name}} - {{tools.splice(item.ar)}}
      </text>
    </block>
  </view>
</view>
